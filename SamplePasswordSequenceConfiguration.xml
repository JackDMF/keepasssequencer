<?xml version="1.0" encoding="utf-8" ?>
<PasswordSequenceConfiguration xmlns="http://quasivirtuel.com/PasswordSequenceConfiguration.xsd">
  <DefaultWords>
    Use Your Own Words
  </DefaultWords>
  <DefaultCharacters>
    1 2 3 4 5 2 6 7 8 9 0 ! " / $ % ? * ( ) _ + | #
  </DefaultCharacters>
  <DefaultSubstitutions>
    <SubstituteWhole replace="here" with="H8"/>
    <SubstituteAny replace="aA" with="4"/>
  </DefaultSubstitutions>
  <Sequence>
    <Word probability="10" capitalize="80">
      <Words>
        Custom Word List
      </Words>
    </Word>
    <Characters probability="always" />
    <Word />
    <Characters>
      <Characters>
        { } [ ] &lt; &gt; _ - = +
      </Characters>
    </Characters>
  </Sequence>
</PasswordSequenceConfiguration>